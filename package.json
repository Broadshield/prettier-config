  },
  "dependencies": {
    "@prettier/plugin-ruby": "^2.0.0",
    "@prettier/plugin-xml": "^2.0.1",
    "prettier": "^2.6.2",
    "prettier-plugin-java": "1.6.1",
    "prettier-plugin-properties": "^0.1.0",
    "prettier-plugin-sh": "^0.10.0"
  },
  "peerDependencies": {
    "@prettier/plugin-ruby": "^2.0.0",
    "@prettier/plugin-xml": "^2.0.1",
    "prettier": "^2.6.2",
    "prettier-plugin-java": "1.6.1",
    "prettier-plugin-properties": "^0.1.0",
    "prettier-plugin-sh": "^0.10.0"
  }
